  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration development --disable-host-check --host 0.0.0.0",
    "dev": "ng serve --configuration dev --disable-host-check --host 0.0.0.0",
    "prod": "ng serve --configuration production --disable-host-check --host 0.0.0.0",
    "kill": "kill -9 $(lsof -t -i:3010)",
    "deploy": "firebase deploy --only hosting",
    "build": "ng build --configuration=production && cd hosting/prod && firebase deploy --only hosting",
    "build:dev": "ng build --configuration=dev && cd hosting/dev && firebase deploy --only hosting",
    "watch": "ng build --watch --configuration development",
    "hosting": "npm run build && npm run build:dev",
    "test": "ng test",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
  },

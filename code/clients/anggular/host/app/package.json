  "license": "https://themeforest.net/licenses/standard",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration development --disable-host-check --host 0.0.0.0 --port 8000",
    "dev": "ng serve --configuration dev --disable-host-check --host 0.0.0.0",
    "prod": "ng serve --configuration production --disable-host-check --host 0.0.0.0",
    "kill": "kill -9 $(lsof -t -i:8000)",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "hosting": "ng build --configuration production && firebase deploy --only hosting && rm -rf dist/",
    "test": "ng test",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
  },
